<Application x:Class="WallPaperGenerator.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:vm="clr-namespace:WallPaperGenerator.ViewModels"
             xmlns:views="clr-namespace:WallPaperGenerator.Views">
    <Application.Resources>
        <DataTemplate DataType="{x:Type vm:MainViewModel}">
            <views:InitialView />
        </DataTemplate>
        <DataTemplate DataType="{x:Type vm:HomeViewModel}">
            <views:InitialView />
        </DataTemplate>
        <DataTemplate DataType="{x:Type vm:PastImagesViewModel}">
            <views:PastImagesView />
        </DataTemplate>
    </Application.Resources>
</Application>